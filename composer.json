{
    "name": "gladehq/laravel-query-lens",
    "description": "A Laravel package for analyzing and optimizing database queries with performance insights",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "database",
        "query",
        "analyzer",
        "profiler",
        "performance",
        "optimization",
        "sql",
        "eloquent"
    ],
    "authors": [
        {
            "name": "Dulitha Rajapaksha",
            "organization": "GladeHQ",
            "email": "dulithamahishka94@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GladeHQ\\QueryLens\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GladeHQ\\QueryLens\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GladeHQ\\QueryLens\\QueryLensServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
